extends base.jade

block css
	link(rel='stylesheet' href='/css/results.css' type='text/css')

block scripts
	script(src='//ajax.googleapis.com/ajax/libs/angularjs/1.3.16/angular.min.js')
	script(src='/js/results/app.js')

block content
	- var currentSection = 'results'
	.master-container
		.headline
			block headline

		.content-wrapper
			main.results-wrapper(ng-app='resultsDisplay')
				block results
			include nav-sidebar
			
