	.data-container(ng-repeat='event in ciclo.evento | limitTo: 1')
		.cell
			h6 sede
			p {{event.sede}}
		.cell
			h6 evento
			p {{event.evento}}
		.cell
			h6 modalidad
			p {{event.modalidad}}
		.cell
			h6 categoría
			p {{event.categoria}}
		.cell
			h6 atletas
			p {{event.atletas}}
		.cell
			h6 entrenadores
			p {{event.entrenadores}}
		.cell
			h6 oficiales
			p {{event.oficiales}}
		.cell
			h6 oro
			p {{event.oro}}
		.cell
			h6 plata
			p {{event.plata}}
		.cell
			h6 bronce
			p {{event.bronce}}
